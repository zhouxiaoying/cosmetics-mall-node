<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>品牌故事</title>
    <link rel="stylesheet" href="static/css/bootstrap.min.css">
    <script type="text/javascript" src="static/js/jquery.min.js"></script>
    <script type="text/javascript" src="static/js/jquery.cookie.js"></script>
    <script type="text/javascript" src="static/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="static/js/jquery.SuperSlide.2.1.3.js"></script>
    <link rel="stylesheet" href="static/css/index.css">
    <style>
        img{
            width: 100%;
            height: 100%;
        }
    </style>
</head>
<body >
<div id="header"></div>
<div class="story">
    <div>
        <img src="static/images/PC_01.jpg" alt="">
    </div>
    <div>
        <img src="static/images/PC_02.jpg" alt="">
    </div>
    <div>
        <img src="static/images/PC_03.jpg" alt="">
    </div>
    <div>
        <img src="static/images/PC_04.jpg" alt="">
    </div>
    <div>
        <img src="static/images/PC_05.jpg" alt="">
    </div>
</div>



<div id="footer"></div>
<script type="text/javascript">
    $(document).ready(function(){
        $('#footer').load('footer.html');
        $('#header').load('header.html');
//        $('#center').load('center.html');
        var userName = $.cookie('userName');
        setTimeout((function(){
            if(userName && userName!=''){
                $('.logina').hide();
                $('.loginname').html(userName).show();
                $('.exita').show();
                if(userName == 'admin'){
                    $('.managercenter').show();
                }
            }
        }),100);
        setTimeout((function() {
            jQuery('.navUl li').removeClass('active');
            jQuery('.navUl li:eq(3)').addClass('active');
        }),100);


 setTimeout((function(){
         var nums = $.cookie('total_num');
                    var prices = $.cookie('total_price');
                    if(nums != null){
                        $('.num-show').text(nums);
                        $('.totalprice').text(prices);
                               }
}),500);

    })
</script>
</body>
</html>